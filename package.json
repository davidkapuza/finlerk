    "typeorm": "env-cmd ts-node --project apps/api/tsconfig.app.json ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=apps/api/src/lib/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=apps/api/src/lib/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=apps/api/src/lib/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=apps/api/src/lib/database/data-source.ts schema:drop",
    "seed:run:relational": "ts-node --project apps/api/tsconfig.app.json -r tsconfig-paths/register apps/api/src/lib/database/seeds/relational/run-seed.ts",
    "prebuild": "rimraf dist",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
